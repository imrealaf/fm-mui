{
  "name": "fm-mui-example",
  "homepage": ".",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "react-app-rewired start --scripts-version react-scripts",
    "build": "node ../node_modules/react-scripts/bin/react-scripts.js build",
    "test": "node ../node_modules/react-scripts/bin/react-scripts.js test",
    "eject": "node ../node_modules/react-scripts/bin/react-scripts.js eject",
    "emulate": "firebase emulators:start --import=\"./local-data\" --export-on-exit=\"./local-data\""
  },
  "dependencies": {
    "@emotion/react": "link:../node_modules/@emotion/react",
    "@emotion/styled": "link:../node_modules/@emotion/styled",
    "@mui/icons-material": "link:../node_modules/@mui/icons-material",
    "@mui/material": "link:../node_modules/@mui/material",
    "@mui/x-date-pickers": "^5.0.18",
    "@reduxjs/toolkit": "^1.9.2",
    "@testing-library/jest-dom": "link:../node_modules/@testing-library/jest-dom",
    "@testing-library/react": "link:../node_modules/@testing-library/react",
    "@testing-library/user-event": "link:../node_modules/@testing-library/user-event",
    "@types/jest": "link:../node_modules/@types/jest",
    "@types/node": "link:../node_modules/@types/node",
    "@types/react": "link:../node_modules/@types/react",
    "@types/react-dom": "link:../node_modules/@types/react-dom",
    "bytes": "^3.1.2",
    "firebase": "^9.17.1",
    "fm-mui": "link:../",
    "moment": "^2.29.4",
    "path-browserify": "^1.0.1",
    "react": "link:../node_modules/react",
    "react-dom": "link:../node_modules/react-dom",
    "react-redux": "^8.0.5",
    "react-scripts": "link:../node_modules/react-scripts",
    "react-transition-group": "^4.4.5",
    "swiper": "^9.0.5",
    "typescript": "link:../node_modules/typescript",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@types/bytes": "^3.1.1",
    "react-app-rewired": "^2.2.1"
  },
  "eslintConfig": {
    "rules": {
      "dot-notation": "off",
      "no-unused-vars": "off",
      "no-async-promise-executor": "off",
      "prefer-promise-reject-errors": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
